{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "togglemic",
  "title": "ToggleMic",
  "description": "Toggle Audio Inpute",
  "icon": "extension_icon.png",
  "author": "nakamuloud",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "menu",
      "title": "Show input volume in menubar",
      "description": "Template for showing a menu bar extra",
      "mode": "menu-bar",
      "interval": "10s"
    },
    {
      "name": "toggle",
      "title": "Show input volume in menubar",
      "description": "Template for showing a menu bar extra",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.2",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
